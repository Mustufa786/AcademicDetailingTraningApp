    <string name="gds02_04b">	0.8mg/kg diazepam injection solution per rectum</string>
    <string name="gds02_04c">	1.0mg/kg diazepam injection solution per rectum</string>
    <string name="gds02_04d">	1.5mg/kg diazepam injection solution per rectum</string>

