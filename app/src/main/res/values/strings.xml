    <!--GDS General Danger Sign 01-->
    <!--GDS01_01-->
    <string name="gds01_01">Children with _________________ on the IMNCI chart usually need urgent referral to hospital.</string>
    <string name="gds01_01a">Mild illness</string>
    <string name="gds01_01b">Pneumonia</string>
    <string name="gds01_01c">Dehydration</string>
    <string name="gds01_01d">Severe disease/classification</string>

    <!--GDS01_02-->
    <string name="gds01_02">_________________is the exception for severe classification or severe disease where referral to the hospital is needed but not urgent.</string>
    <string name="gds01_02a">Persistent diarrhea</string>
    <string name="gds01_02b">Severe dehydration</string>
    <string name="gds01_02c">Severe persistent diarrhea</string>
    <string name="gds01_02d">Severe pneumonia</string>

    <!--GDS01_03-->
    <string name="gds01_03">Presence of even __________ danger sign indicates that underlying disease is severe</string>
    <string name="gds01_03a">One</string>
    <string name="gds01_03b">Two</string>
    <string name="gds01_03c">Three</string>
    <string name="gds01_03d">None</string>

    <!--GDS01_04-->
    <string name="gds01_04">For assessing a general danger sign, a healthcare provider needs to ASK _____ questions and LOOK for ______ observations to observe the child\'s actions.</string>
    <string name="gds01_04a">One, Two</string>
    <string name="gds01_04b">Two, Two</string>
    <string name="gds01_04c">Three, Two</string>
    <string name="gds01_04d">Three, One</string>

    <!--GDS01_05-->
    <string name="gds01_05">Which of the following is NOT a danger sign?</string>
    <string name="gds01_05a">Child is not able to drink or breastfeed</string>
    <string name="gds01_05b">Child vomits everything</string>
    <string name="gds01_05c">Child has had more than one convulsion or prolonged convulsion, or is convulsing now.</string>
    <string name="gds01_05d">Child is shivering</string>

    <!--GDS General Danger Sign 02-->
    <!--GDS02_01-->
    <string name="gds02_01">A classification in a Yellow row (IMNCI chart) is a classification of very severe disease and needs urgent attention and referral.</string>

    <!--GDS02_02-->
    <string name="gds02_02">Which of the following is NOT true, if a child has a general danger sign?</string>
    <string name="gds02_02a">	Complete assessment immediately - the child has a severe problem. </string>
    <string name="gds02_02b">	Provide urgent pre-referral treatment </string>
    <string name="gds02_02c">	Observe and reassess child after pre-referral treatment</string>
    <string name="gds02_02d">	Refer child to hospital</string>

    <!--GDS02_03-->
    <string name="gds02_03">Which of the following is NOT true, if a child has a general danger sign?</string>
