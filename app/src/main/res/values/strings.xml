    <!--CDB Cough & Difficult Breathing 01-->
    <!--CDB01_01-->
    <string name="cdb01_01">Cough or difficult breathing that lasts for more than 14 days may indicate ______________.</string>
    <string name="cdb01_01a">Tuberculosis</string>
    <string name="cdb01_01b">Asthma</string>
    <string name="cdb01_01c">Whooping cough</string>
    <string name="cdb01_01d">All of the above</string>

    <!--CDB01_02-->
    <string name="cdb01_02">Talha is 6 months old. He has fast breathing if:</string>
    <string name="cdb01_02a">30 or more breaths/minute</string>
    <string name="cdb01_02b">40 or more breaths/minute</string>
    <string name="cdb01_02c">50 or more breaths/minute</string>
    <string name="cdb01_02d">60 or more breaths/minute</string>

    <!--CDB01_03-->
    <string name="cdb01_03">Fast breathing or ____________ if present in a child between 2 months to 5 years of age are the clinical signs for Pneumonia.</string>
    <string name="cdb01_03a">Wheeze</string>
    <string name="cdb01_03b">Chest in-drawing</string>
    <string name="cdb01_03c">Cough</string>
    <string name="cdb01_03d">Fever</string>

    <!--CDB01_04-->
    <string name="cdb01_04">The child has chest in drawing if the lower chest wall (lower ribs) goes ____ when the child breathes IN.</string>
    <string name="cdb01_04a">IN</string>
    <string name="cdb01_04b">OUT</string>

    <!--CDB Cough & Difficult Breathing 02-->
    <!--CDB02_01-->
    <string name="cdb02_01">What is the dose of Amoxicillin DT (250 gm) for children aged 12 months up to 3 years?</string>
    <string name="cdb02_01a">(&#189;) tablet twice a day</string>
    <string name="cdb02_01b"> <b>1</b> tablet twice a day</string>
    <string name="cdb02_01c"> <b>2</b> tablet twice a day</string>
    <string name="cdb02_01d"> <b>2.5</b> tablet twice a day</string>

    <!--CDB02_02-->
    <string name="cdb02_02">The dose for intramuscular antibiotics (urgent referral); Ampicillin _______mg/kg and Gentamicin ________mg/kg</string>
    <string name="cdb02_02a"> 40 mg and 7 mg</string>
    <string name="cdb02_02b"> 45 mg and 7.5 mg</string>
    <string name="cdb02_02c"> 50 mg and 7.5 mg</string>
    <string name="cdb02_02d"> 55 mg and 8 mg</string>

    <!--CDB02_03-->
    <string name="cdb02_03">What is the minimum SpO2 saturation level for urgent referral to a health facility?</string>
    <string name="cdb02_03a"> ≥90%</string>
    <string name="cdb02_03b"> ≤90%</string>
    <string name="cdb02_03c"> ≥80%</string>
    <string name="cdb02_03d"> ≥70%</string>

    <!--CDB02_04-->
    <string name="cdb02_04">Standard flow rates for oxygen through nasal prongs or nasal catheters are up to ____ L/min for infants, up to _____ L/min for older children</string>
    <string name="cdb02_04a"><![CDATA[2 & 4]]></string>
    <string name="cdb02_04b"><![CDATA[3 & 4]]></string>
    <string name="cdb02_04c"><![CDATA[3.5 & 4.5]]></string>
    <string name="cdb02_04d"><![CDATA[4 & 4.5]]></string>

    <!--CDB02_05-->
    <string name="cdb02_05">Following of the vaccines prevent pneumonia infections, <b>EXCEPT:</b></string>
